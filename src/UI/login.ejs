<body>

  <!-- ======= Header ======= -->
  <header id="header">

    <div id="topbar">
      <div class="container">
        <div class="social-links">
          <a href="https://github.com/shawnlcwang/APP_Ownagon" class="github"><i class="fa fa-github"></i></a>  
          <a href="https://www.linkedin.com/in/shawn-wang-48801066/" class="linkedin"><i class="fa fa-linkedin"></i></a>
          <a href="https://twitter.com/shawnlcwang" class="twitter"><i class="fa fa-twitter"></i></a>
          <a href="https://www.facebook.com/shawn.wang.3576" class="facebook"><i class="fa fa-facebook"></i></a>
        </div>
      </div>
    </div>

    <div class="container">

      <div class="logo float-left">
        <!-- Uncomment below if you prefer to use an image logo -->
        <h1 class="text-light"><a href="#intro" class="scrollto"><span><%=title%></span></a></h1>
        <!-- <a href="#header" class="scrollto"><img src="assets/img/logo.png" alt="" class="img-fluid"></a> -->
      </div>

      <nav class="main-nav float-right d-none d-lg-block">
        <ul>
          <li class="active"></li>
          <%for(let i=0; i < nav.length; i++){%>
          <li><a href="<%=nav[i].link%>"><%=nav[i].title%></a></li>
          <%}%>
        </ul>
      </nav><!-- .main-nav -->

    </div>
  </header><!-- #header -->

  <!-- ======= Hero Section ======= -->
  <section id="hero" class="clearfix">
    <div class="container d-flex h-100">
      <div class="row justify-content-center align-self-center">
        <div class="col-md-6 intro-info order-md-first order-last">
          <h2><span>Log In</span></h2>
          <div class="form">
            <p>Welcome back!</p>
            <!-- <form action="/auth/signin" method="POST" role="form" class="php-email-form"> -->
            <form action="/auth/login" method="POST" role="form">
              <div class="form-group">
                <input type="email" class="form-control" name="username" id="username" placeholder="User Email" data-rule="email" data-msg="Please enter a valid email" />
                <div class="validate"></div>
              </div>
              <div class="form-group">
                <input type="text" class="form-control" name="password" id="password" placeholder="Password" data-rule="minlen:8" data-msg="Please enter at least 8 characters for password" />
                <div class="validate"></div>
              </div>
              <div class="text-center"><button type="submit" title="Send Message">Confirm</button></div>
            </form>
          </div>
        </div>

        <div class="col-md-6 intro-img order-md-last order-first">
          <img src="/assets/img/intro-img.svg" alt="" class="img-fluid">
        </div>
      </div>

    </div>
  </section><!-- End Hero -->

  <!-- ======= Footer ======= -->
  <footer id="footer" class="section-bg">
    <div class="container">
      <div class="copyright">
        &copy; Copyright <strong>Rapid</strong>. All Rights Reserved
      </div>
